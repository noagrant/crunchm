<div class="row">

	<table class='table table-striped'>
<%# if name == :image_url  %>
<!-- <td></td><td><img src= <%#= value %>></td> -->
		<% @crunchm[:tributes_all_hash].each do |tribute, stats| %>
			<tr><td><%= tribute.to_s %></td>
				<td><%#= stats[:weight] %></td> <!--this will be the slider -->
				<td><%= stats[:placement]%><td>

			
			<% @crunchm.each do |asin,values| #unless asin == tributes_all_hash %>
				<% unless asin == :tributes_all_hash   %>
					<% if values.has_key?(tribute) && tribute != :image_url %>
						<td><%= values[tribute][:value] %></td>
						<td><%= values[tribute][:score] %></td><!--this will be the slider -->
					<% else %>
						<td>--</td>
						<td>--</td>
					<% end %>
				<% end %>
			<% end %>
			</tr>
		<% end %>
	</table>

	
